{
  "name": "@outlast/apiserver",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register 'src/**/*.test.ts'",
    "test:integration": "mocha -r ts-node/register --exclude 'src/**/*.test.ts' 'src/**/*.integration.ts'",
    "test:all": "npm run test && npm run test:integration",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "db:reset": "ts-node -r dotenv/config scripts/db-reset.ts DOTENV_CONFIG_PATH=../../.env",
    "db:seed": "ts-node -r dotenv/config scripts/db-seed.ts DOTENV_CONFIG_PATH=../../.env",
    "start": "node -r dotenv/config dist/index.js DOTENV_CONFIG_PATH=../../.env",
    "dev": "ts-node -r dotenv/config src/index.ts DOTENV_CONFIG_PATH=../../.env"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "express": "^4.18.2",
    "node-cron": "^4.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/sinon": "^17.0.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
